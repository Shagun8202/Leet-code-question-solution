class Solution {
public:
    vector<int> asteroidCollision(vector<int>& arr) {
      vector<int> ans;
      stack<int> st;
      int n = arr.size();
      for(int i =0;i<n;i++){
          if(arr[i]>0){
              st.push(arr[i]);
          }
          else{
              while(!st.empty() && st.top()>0 &&st.top()< -arr[i]){
                  st.pop();
              }
              //st.push(arr[i]);
              if(!st.empty() &&st.top()==-arr[i]){
                  st.pop();
              }
              else if(!st.empty()&&st.top()> -arr[i]){

              }
              else{
                  st.push(arr[i]);
              }

          }
         
      }
       while(!st.empty()){
              ans.push_back(st.top());
              st.pop();
          }
          reverse(ans.begin(),ans.end());
          return ans;
    }
};
